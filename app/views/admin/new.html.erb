<div class = "container">
	<div class = "row">
		<div class="col-md-12">
			<%= form_tag 'process_csv', :multipart => true do %>
			<label for="file">Upload a CSV File</label><br><br> 
			<%= file_field_tag "file", accept: 'text/csv', :class => 'btn btn-default' %><br>
			<%= submit_tag "Upload", :class => 'btn btn-success' %>
			<% end %>
		</div>
	</div>
</div>


